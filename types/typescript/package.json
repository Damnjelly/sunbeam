{
    "name": "sunbeam-types",
    "version": "0.10.2",
    "description": "Types for sunbeam",
    "homepage": "https://pomdtr.github.io/sunbeam",
    "repository": {
        "url": "https://github.com/pomdtr/sunbeam"
    },
    "files": [
        "index.d.ts"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "compile-types": "node compile-types.js",
        "generate-docs": "typescript-documentation --entry ./types.ts --output ../../docs/types.md",
        "compile": "tsc",
        "build": "npm run compile-types && npm run compile",
        "docs": "npm run compile-types && npm run generate-docs"
    },
    "devDependencies": {
        "json-schema-to-typescript": "^12.0.0",
        "typescript": "^5.1.3",
        "typescript-documentation": "^3.0.2"
    }
}
