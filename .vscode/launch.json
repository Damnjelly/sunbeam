{
    "configurations": [
        {
            "name": "sunbeam",
            "type": "go",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}",
        },
        {
            "name": "sunbeam extension browse",
            "type": "go",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}",
            "args": [
                "extension",
                "browse",
            ]
        },
        {
            "name": "run examples",
            "type": "go",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}",
            "args": [
                "read",
                "./sunbeam.yml"
            ]
        }
    ]
}
