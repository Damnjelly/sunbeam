{
    "$schema": "../../../schemas/schema.json",
    "type": "list",
    "items": [
        {
            "title": "Static List",
            "actions": [
                {
                    "type": "push",
                    "page": "./static-list.json"
                }
            ]
        },
        {
            "title": "Dynamic List",
            "actions": [
                {
                    "type": "run",
                    "command": "./dynamic-list.py",
                    "onSuccess": "push"
                }
            ]
        },
        {
            "title": "Dynamic List with Args",
            "actions": [
                {
                    "type": "run",
                    "command": "./dynamic-list.py",
                    "args": [
                        "/"
                    ],
                    "onSuccess": "push"
                }
            ]
        },
        {
            "title": "Full-Blown File Browser",
            "actions": [
                {
                    "type": "run",
                    "onSuccess": "push",
                    "command": "../examples/file-browser/file-browser.py"
                }
            ]
        }
    ]
}
